<Window x:Class="NesuCentre.MainContainerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing"
        xmlns:nesucentre="clr-namespace:NesuCentre"
        Title="Window2" Height="400" Width="400"
        AllowsTransparency="True" 
        OpacityMask="White" 
        WindowStyle="None" 
        Background="Transparent"
        Loaded="Window_Loaded"
        Topmost="True"
        MouseMove="Window_MouseMove"
        KeyDown="Window_KeyDown">
    <Grid>
            <!--<Rectangle Width="20" Height="500" Fill="Aqua"/>-->
        <Canvas Name="C_Canvas">
            <nesucentre:MainNode x:Name="C_MainControl" Height="100" Width="100"/>
        </Canvas>
    </Grid>

</Window>
